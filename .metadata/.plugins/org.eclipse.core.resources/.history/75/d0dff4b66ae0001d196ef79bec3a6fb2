package iut.gon.tp3;

import javafx.geometry.Pos;
import javafx.scene.control.Label;
import javafx.scene.input.MouseEvent;

public class GrilleModel {
	private int taille_tab = 3;
	private String[][] messages = new String[taille_tab][taille_tab];
	
	public GrilleModel() {
		for (int lg = 0; lg < this.taille_tab; lg++) {
			for (int col = 0; col < this.taille_tab; col++) {
				this.messages[col][lg] = String.format("L%dC%d", lg, col));
			}
		}
	}

}
